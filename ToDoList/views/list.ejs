<%- include("header") %> 
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    

    
    <div class="box">
        <% for( task in addTask ) { %>
        <div class="item">
            <input type="checkbox">
            <p><%= addTask[task] %> </p>
            
        </div>
        <% } %>
        <form action="/" method="post" class="item">
            <input type="text" name="newTask" placeholder="Task" autocomplete="off">
            <button type="submit" name="list" value="<%= listTitle %>">+</button>
        </form>
        
    </div>
<%- include("footer") %> 